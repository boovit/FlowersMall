@import "../../css/base.wxss";
@import "../../template/common_template.wxss";
@import "../../template/card_style_template.wxss";

.coupon-list-block{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
}

.coupon-list-block .list-tab{
  display: flex;
  width: 100%;
  height: 93rpx;
}

.coupon-list-block>scroll-view{
  display: flex;
  flex-direction: column;
  width: 100%;
  background-color: #f6f6f6;
}

.coupon-list-block .coupon-content{
  display: flex;
  width: 100%;
  height: auto;
  justify-content: center;
  margin-top: 30rpx;
}
